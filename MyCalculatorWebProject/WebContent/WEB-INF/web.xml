<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>MyCalculatorWebProject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>MyCalculator</display-name>
    <servlet-name>MyCalculator</servlet-name>
    <servlet-class>com.sujata.controllers.MyCalculatorServletClass</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyCalculator</servlet-name>
    <url-pattern>/calculate</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MyOutput</display-name>
    <servlet-name>MyOutput</servlet-name>
    <servlet-class>com.sujata.controllers.MyOutputServletClass</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyOutput</servlet-name>
    <url-pattern>/result</url-pattern>
  </servlet-mapping>
</web-app>