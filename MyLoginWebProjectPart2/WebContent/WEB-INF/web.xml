<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>MyLoginWebProjectPart2</display-name>
  <welcome-file-list>
    <welcome-file>login.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>MyLogin</display-name>
    <servlet-name>MyLogin</servlet-name>
    <servlet-class>com.sujata.controller.MyLoginServletClass</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyLogin</servlet-name>
    <url-pattern>/signin</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MySuccess</display-name>
    <servlet-name>MySuccess</servlet-name>
    <servlet-class>com.sujata.view.MySuccessServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MySuccess</servlet-name>
    <url-pattern>/success</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MyFailure</display-name>
    <servlet-name>MyFailure</servlet-name>
    <servlet-class>com.sujata.view.MyFailureServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MyFailure</servlet-name>
    <url-pattern>/failure</url-pattern>
  </servlet-mapping>
</web-app>